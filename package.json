{
  "name": "react-datepicker",
  "version": "1.0.0",
  "description": "A simple datepicker for react.",
  "main": "app/js/datepicker.js",
  "scripts": {
    "test": "test",
    "watch": "npm run demo-js",
    "demo-js": "watchify -t [ babelify --presets [ es2015 react ] ] demo/main.jsx -o demo/bundle.js",
    "build": "babel --presets react es2015 datepicker/ --out-dir build/js/",
    "css": "node-sass --include-path scss app/scss/main.scss   build/css/main.css"
  },
  "keywords": [
    "datepicker",
    "react"
  ],
  "author": "George Mayer",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "reactify": "^1.1.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "moment": "^2.11.0"
  }
}
