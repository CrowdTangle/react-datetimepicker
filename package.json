{
  "name": "react-datetimepicker",
  "version": "1.0.1",
  "description": "A simple datepicker for react.",
  "main": "build/js/datepicker.js",
  "scripts": {
    "prepublish": "build",
    "test": "jest",
    "watch": "npm run watch:js && npm run watch:css",
    "watch:js": "watchify -t [ babelify --presets [ es2015 react stage-0 ] ] --extension=.jsx demo/main.jsx -o demo/bundle.js -dv",
    "build": "npm run build:js && npm run build:css",
    "build:js": "babel --presets react,es2015,stage-0 app/js/ --out-dir build/js/",
    "build:css": "npm run build:sass && npm run build:postcss",
    "build:sass": "node-sass app/scss/main.scss build/css/main.css",
    "build:postcss": "postcss --use autoprefixer build/css/*.css -d build/css/",
    "watch:css": "node-sass --watch app/scss/main.scss demo/main.css"
  },
  "keywords": [
    "datepicker",
    "datetimepicker",
    "timepicker",
    "react"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "extensify": {
    "extensions": ["jsx"]
  },
  "jest": {
    "collectCoverage": true,
    "testPathDirs": [
      "./__tests__"
    ],
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testFileExtensions": [
      "jsx",
      "js"
    ],
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-dom",
      "./node_modules/react-addons-test-utils"
    ],
    "modulePathIgnorePatterns": [
      "./node_modules/"
    ]
  },
  "author": "George Mayer",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-jest": "^6.0.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "extensify": "^1.0.2",
    "jest": "^0.1.40",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "react-addons-test-utils": "^15.4.0",
    "reactify": "^1.1.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "moment": "^2.11.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0"
  }
}
